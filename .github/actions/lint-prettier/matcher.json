{
  "problemMatcher": [
    {
      "owner": "prettier",
      "pattern": [
        {
          "regexp": "\\[(warn)\\] (.+) (File is not properly formatted.)",
          "severity": 1,
          "file": 2,
          "message": 3
        },
        {
          "regexp": "\\[(error)\\] (.+?): (.+) \\((\\d+):(\\d+)\\)",
          "severity": 1,
          "file": 2,
          "message": 3,
          "line": 4,
          "column": 5
        }
      ]
    }
  ]
}
