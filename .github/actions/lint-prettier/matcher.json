{
  "problemMatcher": [
    {
      "owner": "prettier-warn",
      "pattern": [
        {
          "regexp": "\\[warn\\] (.+) (File is not properly formatted.)",
          "file": 1,
          "message": 2
        }
      ]
    },
    {
      "owner": "prettier-error",
      "pattern": [
        {
          "regexp": "\\[error\\] (.+?): (.+) \\((\\d+):(\\d+)\\)",
          "file": 1,
          "message": 2,
          "line": 3,
          "column": 4
        }
      ]
    }
  ]
}
