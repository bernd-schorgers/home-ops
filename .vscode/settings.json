{
    "files.associations": {
        "**/ansible/**/*.yml": "ansible",
        "**/ansible/**/*.yaml": "ansible"
    },
    "yaml.schemas": {
        "Kubernetes": "deployments/*.yaml"
    },
    "ansible.ansibleLint.path": ".direnv/python-3.9.4/bin/ansible-lint",
    "ansible.ansible.path": ".direnv/python-3.9.4/bin/ansible",
    "ansible.python.interpreterPath": ".direnv/python-3.9.4/bin/python",
    "ansible.ansibleLint.arguments": "-c .github/lint/.ansible-lint",
    "prettier.configPath": ".github/lint/.prettierrc.yaml",
    "prettier.ignorePath": ".github/lint/.prettierignore",
}
